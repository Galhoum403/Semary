{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/Semary_Catalog.pdf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/assets/img(.*)",
      "has": [
        {
          "type": "header",
          "key": "referer",
          "value": "^(?!https://semarysteel.com).*"
        }
      ],
      "destination": "/no-access.jpg",
      "permanent": false
    }
  ]
}
